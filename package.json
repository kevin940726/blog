{
  "private": true,
  "name": "blog",
  "homepage": "https://kevin940726.github.io/blog",
  "twitter": "",
  "repository": "git@github.com:kevin940726/blog.git",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"web_modules/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build",
    "predeploy": "npm run build",
    "deploy": "./scripts/deploy.sh"
  },
  "phenomic": {
    "CNAME": true
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "max-len": [
        1,
        120
      ]
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "css-loader": "^0.23.0",
    "cssx-loader": "^5.2.0",
    "eslint": "^2.0.0",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "history": "^2.0.0",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.16.2",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.7.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "react-dom": "^0.14.0 || ^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.12.3",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "dateformat": "^1.0.12",
    "highlight.js": "^9.6.0",
    "jss": "^5.4.0",
    "jss-preset-default": "^0.2.0",
    "react-addons-shallow-compare": "^15.3.1",
    "react-virtualized": "^7.22.2"
  }
}
